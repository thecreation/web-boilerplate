{
  "version": "0.0.5",
  "name": "web-boilerplate",
  "author": "Creation Studio Limited",
  "license": "MIT",
  "repository": "https://github.com/thecreation/web-boilerplate",
  "bugs": {
    "url": "https://github.com/thecreation/web-boilerplate/issues"
  },
  "homepage": "https://github.com/thecreation/web-boilerplate",
  "scripts": {
    "start": "npm-run-all --sequential build --parallel serve watch",
    "dev": "NODE_ENV=dev scripts/dev",
    "build": "npm-run-all img svg favicon icons vendor css:compile js:compile html:compile",
    "dist": "NODE_ENV=prod npm-run-all img svg favicon vendor css js html",
    "clean": "npm-run-all clean:*",
    "clean:css": "scripts/clean-css",
    "clean:js": "scripts/clean-js",
    "clean:img": "scripts/clean-img",
    "clean:html": "scripts/clean-html",
    "clean:svg": "scripts/clean-svg",
    "clean:vendor": "scripts/clean-vendor",
    "clean:favicon": "scripts/clean-favicon",
    "css": "npm-run-all css:beautify css:compile css:minify",
    "css:beautify": "scripts/beautify-css",
    "css:compile": "scripts/compile-css",
    "css:minify": "scripts/minify-css",
    "js": "npm-run-all js:beautify js:compile js:minify",
    "js:beautify": "scripts/beautify-js",
    "js:compile": "scripts/compile-js",
    "js:minify": "scripts/minify-js",
    "icons": "scripts/generate-icons",
    "img": "npm-run-all img:minify",
    "img:minify": "scripts/minify-img",
    "svg": "npm-run-all svg:minify",
    "svg:minify": "scripts/minify-svg",
    "lint": "npm-run-all lint:css:fix lint:js:fix lint:html lint:md",
    "lint:css": "scripts/lint-css",
    "lint:css:fix": "scripts/lint-css --fix",
    "lint:js": "scripts/lint-js",
    "lint:js:fix": "scripts/lint-js --fix",
    "lint:md": "scripts/lint-md",
    "lint:html": "htmllint --rc configs/.htmllintrc \"dist/**/*.html\"",
    "watch": "npm-run-all --parallel watch:*",
    "watch:js": "scripts/watch-js",
    "watch:css": "scripts/watch-css",
    "watch:html": "scripts/watch-html",
    "html": "npm-run-all html:compile html:minify",
    "html:compile": "scripts/compile-html",
    "html:minify": "scripts/usemin",
    "serve": "scripts/serve",
    "serve:reload": "scripts/serve-reload",
    "favicon": "scripts/favicon",
    "vendor": "scripts/vendor",
    "release": "release-it"
  },
  "dependencies": {
    "picturefill": "^3.0.3",
    "vanilla-lazyload": "^10.16.0-beta"
  },
  "browserslist": [
    "> 1%",
    "ie >= 11",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "assets-manager": "^0.3.8",
    "autoprefixer": "^9.3.1",
    "babel-eslint": "^10.0.1",
    "browser-sync": "^2.26.3",
    "chalk": "^2.4.1",
    "colors": "^1.3.2",
    "cssnano": "^4.1.7",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-compat": "^2.6.3",
    "favicons": "^5.2.0",
    "front-matter": "^3.0.1",
    "fs-extra": "^7.0.0",
    "gaze": "^1.1.3",
    "glob-expand": "^0.2.1",
    "glob-parent": "^3.1.0",
    "globby": "^8.0.1",
    "handlebars-helpers": "^0.10.0",
    "handlebars-layouts": "^3.1.4",
    "handlebars-wax": "^6.1.0",
    "helper-dateformat": "^1.0.0",
    "htmllint-cli": "0.0.7",
    "husky": "^1.1.3",
    "image-size": "^0.6.3",
    "imagemin": "^6.0.0",
    "imagemin-giflossy": "^5.1.10",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^6.0.0",
    "imagemin-svgo": "^7.0.0",
    "imagemin-webp": "^4.1.0",
    "jsonlint": "^1.6.3",
    "lint-staged": "^8.0.4",
    "markdownlint": "^0.11.0",
    "node-sass": "^4.10.0",
    "node-sass-import": "^2.0.1",
    "npm-run-all": "^4.1.3",
    "postcss": "^7.0.5",
    "postcss-csso": "^3.0.0",
    "postcss-header": "^1.0.0",
    "postcss-reporter": "^6.0.0",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.14.3",
    "release-it": "^7.6.2",
    "rename": "^1.0.4",
    "rollup": "^0.67.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "semver": "^5.6.0",
    "sharp": "^0.21.0",
    "stringify-attributes": "^1.0.0",
    "stylelint": "^9.7.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.2",
    "svgo": "^1.1.1",
    "uglify-es": "^3.3.9",
    "usemin": "^0.5.0",
    "webfont": "^8.1.4",
    "yargs": "^12.0.2",
    "yargs-parser": "^11.0.0"
  }
}
